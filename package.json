{
  "name": "mlweb-openai-upwork",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "direnv exec . nodemon --exec \"node --expose-gc node_modules/.bin/ts-node src/index.ts\"",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:detailed": "jest --coverage --verbose",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:report": "jest --coverage --coverageReporters=json-summary --outputFile=coverage/coverage-summary.json",
    "lint": "tsc --noEmit",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "test:all": "npm run typecheck && npm run test:coverage"
  },
  "keywords": [
    "ai",
    "agent",
    "nlweb",
    "openai",
    "chatbot"
  ],
  "author": "AI Agent Developer",
  "license": "ISC",
  "description": "Proof-of-concept AI agent integrating NLWeb and OpenAI API for natural language queries",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "openai": "^5.23.0",
    "lucide-react": "^0.544.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/supertest": "^6.0.3",
    "@types/node": "^24.5.2",
    "@types/winston": "^2.4.4",
    "jest": "^30.1.3",
    "joi": "^18.0.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "winston": "^3.17.0"
  }
}
